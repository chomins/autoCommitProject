# Google Gemini 사용 예제
# 이 파일을 .env로 복사하고 실제 API 키를 입력하세요

# AI Provider
AI_PROVIDER=gemini

# Google API Key (https://aistudio.google.com/app/apikey 에서 발급)
GOOGLE_API_KEY=

# 모델 선택 (추천: gemini-2.0-flash)
AI_MODEL=gemini-2.0-flash

# 온도 설정 (0.0 ~ 1.0, 낮을수록 일관적)
AI_TEMPERATURE=0.3

# 최대 토큰 수
AI_MAX_TOKENS=500

